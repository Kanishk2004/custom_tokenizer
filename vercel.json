{
  "version": 2,
  "name": "custom-word-level-tokenizer",
  "routes": [
    {
      "src": "/",
      "dest": "/public/landing.html"
    },
    {
      "src": "/app",
      "dest": "/public/index.html"
    },
    {
      "src": "/tokenizer",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
